<Window x:Class="Client.EditUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditUser" Height="300" Width="300"
        Closing="Window_Closing">
    <Grid>
        <Label Content="Username*:" Margin="10,10,215,226"/>
        <TextBox  x:Name="_Username" Text="username" Margin="89,10,10,226"/>
        <CheckBox  Name="_List" Margin="139,71,71,174">
            List?
        </CheckBox>
        <CheckBox  Name="_Read" Margin="10,71,158,174">
            Read(download)?
        </CheckBox>
        <CheckBox  Name="_Write" Margin="11,116,158,129">
            Write(upload)?
        </CheckBox>
        <CheckBox  Name="_Delete" Margin="139,116,71,129">
            Delete?
        </CheckBox>
        <Button  Click="EditUser_Click" Margin="10,226,170,10">
            Edit User
        </Button>
        <Button  Click="Cancel_Click" Margin="170,226,10,10">
            Cancel
        </Button>
        <Label Margin="10,146,225,84" >
            Home*
        </Label>
        <TextBox Name="_Home" Margin="89,146,50,84">
            /
        </TextBox>
    </Grid>
</Window>
