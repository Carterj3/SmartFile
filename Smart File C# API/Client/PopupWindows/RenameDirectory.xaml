<Window x:Class="Client.RenameDirectory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename Directory" Height="155" Width="350"
        Closing="Window_Closing">
    <Grid>
        <Label Margin="10,10,243,76">
            New Name:
        </Label>
        <TextBox Margin="104,10,10,76" Name="DirectoryName">
            New Name
        </TextBox>
        <Button Margin="10,77,250,10" Name="Rename" Click="Rename_Click">
            Rename
        </Button>
        <Button Margin="104,77,156,10" Name="Cancel" Click="Cancel_Click">
            Cancel
        </Button>
    </Grid>
</Window>
